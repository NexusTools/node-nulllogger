    "compilerOptions": {
        "target": "es6",
	"module": "commonjs",
        "removeComments": true,
        "preserveConstEnums": true,
	"moduleResolution": "node",
        "skipLibCheck": true,
        "sourceMap": true
    },
    "include": [
        "lib/*.ts",
        "lib/impl/*.ts",
        "test/*.ts"
    ]
}
